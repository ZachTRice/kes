  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.14.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "dotenv": "^4.0.0",
    "elasticsearch": "^12.1.3",
    "fs-extra": "^2.0.0",
    "handlebars": "^4.0.6",
    "hapi": "^16.1.0",
    "http-aws-es": "^1.1.3",
    "js-yaml": "^3.8.1",
    "lodash.capitalize": "^4.2.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.replace": "^4.1.4",
    "lodash.startswith": "^4.2.1",
    "lodash.trim": "^4.5.1",
    "lodash.upperfirst": "^4.3.1",
